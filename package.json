{
  "name": "ngx-filesize-root",
  "version": "0.0.0",
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "scripts": {
    "build:ngx-filesize": "yarn workspace ngx-filesize build",
    "build:demos": "yarn workspaces foreach -pv --include \"filesize-*-demo\" run build",
    "build:all": "yarn build:ngx-filesize && yarn build:demos",
    "test:all": "yarn workspaces foreach -pv run test:ci",
    "lint:all": "yarn workspaces foreach -pv run lint"
  },
  "packageManager": "yarn@3.2.3"
}
