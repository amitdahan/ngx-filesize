    ".": {
      "types": "./dist/ngx-filesize/index.d.ts",
      "esm2022": "./dist/ngx-filesize/esm2022/ngx-filesize.mjs",
      "esm": "./dist/ngx-filesize/esm2022/ngx-filesize.mjs",
