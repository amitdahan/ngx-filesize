    "test": "ng test",
    "test:ci": "yarn test --no-watch --no-progress --browsers=ChromeHeadless",
    "lint": "ng lint"
  },
  "peerDependencies": {
    "@angular/common": ">= 16.0.0 < 17.0.0",
